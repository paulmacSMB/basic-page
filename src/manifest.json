{
  "manifest_version": 3,
  "name": "Basic-Page",
  "description": "A webpage mask to simplify webpages",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}

